{
  "name": "Query Param Toggle",
  "short_name": "Querytoggle",
  "description": "Toggle predefined query params",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon":"icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}

{
  "name": "Find Developer Tools",
  "short_name": "Find Developer Tools",
  "description": "Find Developer Tools",
  "version": "1.0.5",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "res/icon.png",
      "32": "res/icon.png",
      "48": "res/icon.png",
      "128": "res/icon.png"
    }
  },
  "options_page": "src/options.html",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs",
    "<all_urls>",
    "cookies",
    "*://*/*"
  ],
  "icons": {
    "16": "res/icon.png",
    "32": "res/icon.png",
    "48": "res/icon.png",
    "128": "res/icon.png"
  }
}

{
  "name": "Find Developer Tools",
  "short_name": "Find Developer Tools",
  "description": "Find Developer Tools",
  "version": "1.0.4",
  "manifest_version": 2,
  "permissions": [
    "cookies",
    "https://shop.pre.rd-ecom-test.com/",
    "https://shop.int.rd-ecom-test.com/",
    "https://shop.rewe.de/",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["style.css"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon":"icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
